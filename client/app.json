{
  "expo": {
    "name": "client",
    "slug": "client",
    "owner": "lihiamir",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/moon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/moon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lihiamir.client"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/moon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/moon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "92734167-b8e8-4fe6-b47f-da1974c13a95"
      }
    }
  }
}
